<Application x:Class="SafeKey.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SafeKey"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="Background" Color="#323232" />
        <SolidColorBrush x:Key="input-background" Color="#3A3A3A" />
        <SolidColorBrush x:Key="safekey-accent" Color="#FF0B55" />
        <SolidColorBrush x:Key="input-text-placeholder" Color="#5A5A5A" />
        <SolidColorBrush x:Key="input-text" Color="#EEEEEE" />
        <SolidColorBrush x:Key="input-border-brush" Color="#303030" />
    </Application.Resources>
</Application>
